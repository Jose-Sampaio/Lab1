//Exemplo Stack 1
#include <iostream>
#include <stack>
using namespace std;

int main()
{
	stack <int> pilha;
	int n;
	for (int i = 0; i < 4; ++i)
	 {
	 	cin>>n;
	 	pilha.push(n);
	 } 

	 while (!pilha.empty()){
	 	cout<<pilha.top()<<endl;
	 	pilha.pop();
	 }
	
	return 0;
}
----------------------------------------------------------------------------------------------------------------------------
//Exemplo de Queue 1
#include <iostream>
#include <queue>
using namespace std;

int main()
{
	queue <int> fila;
	int n;
	for (int i = 0; i < 4; ++i)
	 {
	 	cin>>n;
	 	fila.push(n);
	 } 

	 while (!fila.empty()){
	 	cout<<fila.front()<<endl;
	 	fila.pop();
	 }
	
	return 0;
}

